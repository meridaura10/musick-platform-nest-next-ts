{
  "version": 2,
  "builds": [
    {
      "src": "main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "/index.ts"
    }
  ],
  "env": {
    "EXAMPLE_VAR": "example-value"
  },
  "middlewares": [
    {
      "name": "cors",
      "handler": "./middlewares/cors.js"
    }
  ]
}


