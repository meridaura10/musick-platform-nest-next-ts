{
    "version": 2,
    "routes": [
      {
        "src": "/.*",
        "dest": "index.js"
      }
    ],
    "env": {
      "MY_VARIABLE": "value"
    },
    "build": {
      "env": {
        "BUILD_VARIABLE": "value"
      }
    },
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ]
  }


